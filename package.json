{
  "name": "emancipation-grill",
  "version": "0.0.0",
  "description": "a node client for Hashicorp's Vault",
  "main": "index.js",
  "dependencies": {
    "bluebird": "~2.9.27",
    "lodash": "~3.9.3",
    "request": "~2.57.0",
    "tough-cookie": "~1.2.0"
  },
  "devDependencies": {
    "istanbul": "~0.3.14",
    "jscs": "~1.13.1",
    "mocha": "~2.2.5",
    "must": "~0.12.0"
  },
  "scripts": {
    "lint": "jshint index.js test.js && jscs test/*.js index.js",
    "test": "mocha --check-leaks -R spec test/",
    "coverage": "istanbul cover _mocha test --recursive -- -u exports -R spec && open coverage/lcov-report/index.html",
    "test-travis": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceejbot/emancipation-grill.git"
  },
  "keywords": [
    "hashicorp",
    "vault"
  ],
