{
  "name": "emancipation-grill",
  "version": "0.0.0",
  "description": "a node client for Hashicorp's Vault",
  "main": "index.js",
  "dependencies": {
    "bluebird": "~2.9.30",
    "lodash": "~3.9.3",
    "request": "~2.58.0",
    "tough-cookie": "~2.0.0"
  },
  "devDependencies": {
    "istanbul-harmony": "~0.3.16",
    "jscs": "~1.13.1",
    "mocha": "~2.2.5",
    "must": "~0.12.0"
  },
  "scripts": {
    "lint": "jshint index.js test/*.js && jscs test/*.js index.js",
